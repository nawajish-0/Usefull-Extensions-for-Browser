{
    "manifest_version": 3,
    "name": "Tab Volume Controller",
    "version": "1.0",
    "permissions": ["tabs", "scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  